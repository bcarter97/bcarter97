{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["import", "simple-import-sort"],
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "no-eval": "error",
    "import/first": "error",
    "no-unused-vars": "error",
    "simple-import-sort/imports": "error"
  }
}
